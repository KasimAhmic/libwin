{
  "name": "@libwin/gdi32",
  "version": "1.0.0",
  "description": "Native Node.js bindings for the Win32 API",
  "main": "dist/index.js",
  "type": "module",
  "gypfile": true,
  "scripts": {
    "configure": "node-gyp configure",
    "clean": "node-gyp clean",
    "build:lib": "tsc -p ./tsconfig.json && rollup -c",
    "build": "node-gyp build -j max && npm run build:lib",
    "rebuild": "node-gyp rebuild -j max && npm run build:lib",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KasimAhmic/libwin.git"
  },
  "keywords": [
    "nodejs",
    "native",
    "addon",
    "win32",
    "gdi32",
    "c++"
  ],
  "author": "Kasim Ahmic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KasimAhmic/libwin/issues"
  },
  "homepage": "https://github.com/KasimAhmic/libwin#readme",
  "dependencies": {
    "@libwin/common": "*"
  }
}
